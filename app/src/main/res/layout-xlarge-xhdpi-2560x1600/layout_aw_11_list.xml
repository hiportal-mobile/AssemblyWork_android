<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    style="@style/style_layout_bg"
    android:layout_width="fill_parent"
    android:layout_height="80dp"
    android:baselineAligned="true"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/tv_aw_11_list_title"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/s_unknown"
        android:textColor="#111111"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/tv_aw_11_list_req_content"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:text="@string/s_unknown"
        android:textColor="#111111"
        android:textSize="18sp" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="10dp"
        android:layout_marginTop="10dp"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/tv_aw_11_list_username"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/s_unknown"
            android:textColor="#111111"
            android:textSize="17sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/tv_aw_11_list_self_req"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/s_unknown"
            android:textAppearance="?android:attr/textAppearanceMedium"
            android:textColor="@color/darkgrey" />
    </LinearLayout>

</LinearLayout>