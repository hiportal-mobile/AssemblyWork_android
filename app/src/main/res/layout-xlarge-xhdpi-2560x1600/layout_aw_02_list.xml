<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/tab_pic_lay"
    android:baselineAligned="true"
    android:orientation="horizontal"
    style="@style/style_layout_bg"

	>

    <ImageView
        android:id="@+id/iv_awgt_02_list_assemblyman_img"
       android:layout_width="@dimen/tab_pic_height"
            android:layout_height="@dimen/tab_main_icon_width"
        android:layout_marginLeft="10dip" 
        android:layout_marginRight="15dip" 
        android:layout_marginBottom="10dip"
        android:layout_marginTop="10dip"
        android:scaleType="fitXY"
        android:contentDescription="@string/s_unknown"/>

    <LinearLayout
        android:id="@+id/linearLayout1"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center_vertical"
        android:gravity="center_vertical"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/tv_awgt_02_list_assembly"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/s_unknown"
            android:textStyle="bold"
            style="@style/tv_awgt_02_list_assembly" />

        <TextView
            android:id="@+id/tv_awgt_02_list_assembly_dtl"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dip"
            android:text="@string/s_unknown"
             style="@style/tab_tv_awgt_02_list_assembly_dtl" />
    </LinearLayout>

</LinearLayout>