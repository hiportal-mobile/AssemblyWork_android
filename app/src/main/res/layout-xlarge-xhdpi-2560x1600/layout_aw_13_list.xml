<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout1"
    style="@style/style_layout_bg"
    android:layout_width="fill_parent"
    android:layout_height="80dp"
    android:gravity="center_vertical"
    android:orientation="horizontal" >

    <ImageView
        android:id="@+id/iv_aw_13_list_check_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dip"
        android:background="@drawable/checkbox_off"
        android:layout_gravity="center_vertical"
        android:text="@string/s_unknown" />

    <ImageView
        android:id="@+id/iv_aw_13_list_check_on"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="8dip"
        android:background="@drawable/checkbox_on"
        android:layout_gravity="center_vertical"
        android:text="@string/s_unknown" />

    <TextView
        android:id="@+id/tv_aw_13_list_content"
        android:layout_width="fill_parent"
      android:layout_height="50dp"
	    android:layout_marginLeft="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        android:textColor="#333333"
        android:textSize="18dp"/>

</LinearLayout>