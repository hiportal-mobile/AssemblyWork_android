	VERSION: 0.1
-->
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout1"
    style="@style/style_layout_bg"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal"
    android:gravity="center_vertical"
    android:layout_margin="5dp" >
    

    <ImageView
        android:id="@+id/iv_aw_08_list_check_off"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:background="@drawable/checkbox_off"
        android:layout_gravity="center_vertical"
        android:text="@string/s_unknown" />
    
    <ImageView
        android:id="@+id/iv_aw_08_list_check_on"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:background="@drawable/checkbox_on"
        android:layout_gravity="center_vertical"
        android:text="@string/s_unknown" />
    
	<TextView
	    android:id="@+id/tv_aw_08_list_content"
	    android:gravity="center_vertical"
        android:layout_width="fill_parent"
        android:layout_height="60dp"
        android:layout_marginLeft="15dp"
        style="@style/style_input_text" />

</LinearLayout>